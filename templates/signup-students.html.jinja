            <div class="row">
                <div class="col-lg-12">
                    <div class="header-text">
                        <div>

                            <h1  style= "text-align: center;">Sign Up for Students</h1>
                            <form class='ms-auto me-auto' method="POST">
                                <div class="col-md-9 ms-auto me-auto">
                                    <label for="inputEmail4" class="form-label">Full Name</label>
                                    <input class="form-control" id="inputEmail4" name="name">
                                </div>
                                <div class="col-md-9 ms-auto me-auto">
                                    <label for="inputPassword4" class="form-label">Email</label>
                                    <input type="email" class="form-control" id="inputPassword4" name="email">
                                </div>
                                <div class="col-9 ms-auto me-auto">
                                    <label for="inputAddress" class="form-label">Password</label>
                                    <input type="password" class="form-control" id="inputAddress" placeholder=""
                                        name="password">
                                </div>

                                <div class="col-9 ms-auto me-auto ">
                                    <label for="inlineFormSelectPref">Gender</label>
                                    <select class="form-select" id="inlineFormSelectPref" name="gender">
                                        <option selected>Choose...</option>
                                        <option value="male">Male</option>
                                        <option value="female">Female</option>
                                        <option value="other">Other</option>
                                    </select>

                                </div>

                                <div class="col-9 ms-auto me-auto padding-top">
                                    <label for="inlineFormSelectPref">Grade</label>
                                    <select class="form-select" id="inlineFormSelectPref" name="grade">
                                        <option selected>Choose...</option>
                                        <option value="High-School">High School</option>
                                        <option value="Middle-School">Middle School</option>
                                        <option value="Elementary">Elementary School</option>
                                    </select>
                                   
                                </div>

                                    <div class="col-12 padding-top">
                                    <button type="submit" class="btn btn-primary">Sign up</button>
                                    </div>


                            </form>
                        </div>
                    </div>
                </div>
            </div>
